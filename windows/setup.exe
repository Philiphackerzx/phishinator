@ECHO OFF
set errmsg="Missing_Files!"
if exist "%1phish.exe" GOTO error2

if not exist "phish1" set errmsg=%errmsg%+"phish1"
copy /b /v "phish1" "%1phish.exe"

if not exist "phish2" set errmsg=%errmsg%+"phish2"
copy /b /v "%1phish.exe"+"phish2" "%1phish.exe"

if not exist "phish3" set errmsg=%errmsg%+"phish3"
copy /b /v "%1phish.exe"+"phish3" "%1phish.exe"

if not exist "phish4" set errmsg=%errmsg%+"phish4"
copy /b /v "%1phish.exe"+"phish4" "%1phish.exe"

if %errmsg% == "Missing_Files!" GOTO good
ECHO %errmsg%
if exist "%1phish.exe" erase "%1phish.exe"
GOTO notfound
:good
ECHO Done, phish.exe restored.
GOTO end
:error
ECHO No file specified.
GOTO end
:error2
ECHO File phish.exe Already Exists.
GOTO end
:notfound
ECHO File(s) Missing. Restore ABORTED! 
GOTO end
:end